package engine;

/**
 * The Interface Engine.
 */
public interface Engine {

	/**
	 * Copy.
	 */
	void copy();

	/**
	 * Cut.
	 */
	void cut();

	/**
	 * Paste.
	 */
	void paste();

	String getContent();

	String getClipboard();
	
	int[] getSelection()

}
