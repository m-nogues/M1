package mementos;

import org.apache.logging.log4j.LogManager;
import org.apache.logging.log4j.Logger;

import editor.Recorder;
import engine.MoteurEdition;
import recordables.SupTexteEnregistrable;

/**
 * Cette classe est chargée de stocker l'état d'une commande SupTexteEnregistrable
 * @see SupTexteEnregistrable
 * @see MementoCommande
 */
public final class MementoSupprTexte extends MementoCommande{

	private static final Logger LOGGER = LogManager.getLogger(MementoDelText.class.getName());	
	
	public MementoSupprTexte(MoteurEdition moteur, Recorder recorder){
		
		super(moteur, recorder);
		LOGGER.trace("Création d'un MementoDelText");
	}
}