FUNC VOID main()
{
	// Test de déclaration
	INT x, y, z, a[2]

	// Test d'affectation
	y := 0

	// Test d'affectation d'expression avec variable
	x := 1 + y

	// Test de condition
	IF x THEN
		x := 5
	ELSE
		x := 6
	FI

	// Test d'affectation d'expression dans un tableau
	a[0] := 1
	PRINT a[0], "\n"

	// Test d'affectation de variable dans un tableau
	a[1] := x

	// Test d'affectation d'un tableau dans une variable
	z := a[1]

	// Test d'affichage de variables, de tableau et de texte
	PRINT "Test sample\n", x, "\n", a[1], "\n", z, "\nSecond test sample\n"

	// Test d'affectation d'une variable depuis la fonction système READ
	READ z

	PRINT z
}
